{
  "manifest_version": 2,
  "name"            : "AwesomeO",
  "description"     : "Celebrate the small wins and extend your awesomeness.",
  "short_name"      : "awesome-o",
  "version"         : "0.1",

  "icons": { 
    "32": "images/a32x32.png",
    "48": "images/a48x48.png",
    "128": "images/a128x128.png" 
  },

  "browser_action": {
    "default_popup": "index.html",
    "default_icon" : "images/a32x32.png"
  },

  "permissions": [
    "debugger",
    "storage",
    "unlimitedStorage"
  ]

}
